entity SLogin {
     Username String,
     Password String,
     IsActive Boolean,
     CreateTime ZonedDateTime,
     UpdateTime ZonedDateTime
}

entity SRole {
     RoleName String,
     IsActive Boolean,
     CreateTime ZonedDateTime,
     UpdateTime ZonedDateTime
}

relationship ManyToMany {
     SLogin{role} to SRole{login}

}

service * with serviceImpl
